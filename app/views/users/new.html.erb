<h1>Sign up</h1>
<%= form_with(model: @user, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br>
  <%= f.submit "アカウントを作成する" %>
<% end %>

<%=link_to "ログイン画面へ" %>
